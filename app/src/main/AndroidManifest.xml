<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jeneska.scoreboard">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/sb_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/sb_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LiverpoolActivity"
            android:label="Liverpool FC" />
        <activity
            android:name=".OwlActivity"
            android:label="LA Gladiators" />
        <activity
            android:name=".OwlRosterActivity"
            android:label="Roster" />
        <activity
            android:name=".OwlPlayerActivity"
            android:label="Player Information" />

    </application>

</manifest>